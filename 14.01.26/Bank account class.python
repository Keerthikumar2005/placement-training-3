class Bank:
    def __init__(self):
        self.balance = 0

    def deposit(self, amt):
        self.balance += amt

    def withdraw(self, amt):
        if amt <= self.balance:
            self.balance -= amt
        else:
            print("Insufficient balance")

    def display(self):
        print("Balance:", self.balance)

acc = Bank()
acc.deposit(5000)
acc.withdraw(2000)
acc.display()
